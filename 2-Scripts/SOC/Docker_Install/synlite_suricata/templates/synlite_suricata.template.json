      "number_of_replicas": 1,
      "refresh_interval": "10s",
      "codec": "best_compression"
    }
  },
  "mappings": {
    "_default_": {
      "numeric_detection": true,
      "dynamic_templates": [
        {
          "tcp.ack": {
            "path_match": "tcp.ack",
            "mapping": {
              "type": "boolean"
            }
          }
        },
        {
          "tcp.cwr": {
            "path_match": "tcp.cwr",
            "mapping": {
              "type": "boolean"
            }
          }
        },
        {
          "tcp.ece": {
            "path_match": "tcp.ece",
            "mapping": {
              "type": "boolean"
            }
          }
        },
        {
          "tcp.fin": {
            "path_match": "tcp.fin",
            "mapping": {
              "type": "boolean"
            }
          }
        },
        {
          "tcp.psh": {
            "path_match": "tcp.psh",
            "mapping": {
              "type": "boolean"
            }
          }
        },
        {
          "tcp.rst": {
            "path_match": "tcp.rst",
            "mapping": {
              "type": "boolean"
            }
          }
        },
        {
          "tcp.syn": {
            "path_match": "tcp.syn",
            "mapping": {
              "type": "boolean"
            }
          }
        },
        {
          "tcp.urg": {
            "path_match": "tcp.urg",
            "mapping": {
              "type": "boolean"
            }
          }
        },

        
        {
          "string_fields": {
